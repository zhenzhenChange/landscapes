{
  "name": "cat-admin",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "yarn lint:syntax & yarn lint:format",
    "lint:syntax": "ng lint --fix --cache",
    "lint:format": "prettier ./**/*.{js,ts,md,css,less,scss,yml,yaml,html,json} --write --cache --ignore-unknown",
    "clean": "npx rimraf ./node_modules & yarn clean:cache & yarn clean:build",
    "clean:cache": "npx rimraf ./.angular",
    "clean:build": "npx rimraf ./test ./dist ./stats ./develop ./production",
    "start": "ng serve --hmr",
    "build:pro": "ng build --configuration=production --output-path=production/cat-admin",
    "build:dev": "ng build --configuration=develop --output-path=develop/cat-admin",
    "build:test": "ng build --configuration=develop --output-path=test/cat-admin",
    "build:stats": "ng build --stats-json --output-path=stats & yarn build:analyze",
    "build:analyze": "webpack-bundle-analyzer ./stats/stats.json --port=3721"
  },
  "dependencies": {
    "@angular/animations": "^15.1.4",
    "@angular/common": "^15.1.4",
    "@angular/core": "^15.1.4",
    "@angular/forms": "^15.1.4",
    "@angular/platform-browser": "^15.1.4",
    "@angular/platform-browser-dynamic": "^15.1.4",
    "@angular/router": "^15.1.4",
    "@ant-design/icons-angular": "^15.0.0",
    "core-js": "^3.28.0",
    "date-fns": "^2.29.3",
    "echarts": "^5.4.1",
    "ng-zorro-antd": "^15.0.3",
    "ngx-echarts": "^15.0.1",
    "rxjs": "^7.8.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.1.5",
    "@angular/compiler": "^15.1.4",
    "@angular/compiler-cli": "^15.1.4",
    "@angular/language-service": "^15.1.4",
    "@types/node": "^16.18.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "tslib": "~2.4.1",
    "typescript": "~4.8.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
